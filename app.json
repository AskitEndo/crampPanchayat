    "slug": "cramppanchayat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/app-icon.png",
