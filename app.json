        "foregroundImage": "./src/assets/images/app-icon.png",
        "backgroundColor": "#E91E63"
      },
      "package": "com.cramppanchayat.app"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "6aaf303b-bac1-4572-8a6a-0870bcd5c27b"
